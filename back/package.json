{
  "name": "back",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "npx ttsc && babel dist -d dist",
    "prod": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --files -r tsconfig-paths/register src/server.ts",
    "typeorm": "node --require ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/database/ormconfig.ts",
    "migration:create": "npm run typeorm migration:create",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/multer": "^1.4.7",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "fs": "^0.0.1-security",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.4.3",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.14.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.37",
    "uuid": "^8.3.2"
  }
}
